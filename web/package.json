{
  "name": "immich-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npm run dev:all",
    "dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
    "build:svelte": "vite build",
    "build:electron": "electron-builder --win --config build.config.json",
    "build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
    "dev:electron": "electron src/electron.cjs",
    "dev:svelte": "vite dev --host 0.0.0.0 --port 3000",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check:svelte": "svelte-check --no-tsconfig --fail-on-warnings --ignore \"src/api/open-api\"",
    "check:typescript": "tsc --noEmit",
    "check:watch": "npm run check:svelte -- --watch",
    "check:code": "npm run format && npm run lint && npm run check:svelte && npm run check:typescript",
    "check:all": "npm run check:code && npm run test:cov",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^7.6.0",
    "@floating-ui/dom": "^1.5.1",
    "@sveltejs/adapter-node": "^1.2.0",
    "@sveltejs/kit": "^1.20.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^4.0.3",
    "@types/cookie": "^0.5.1",
    "@types/dom-to-image": "^2.6.4",
    "@types/justified-layout": "^4.1.0",
    "@types/leaflet": "^1.9.1",
    "@types/leaflet.markercluster": "^1.5.1",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.4.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0-beta.5",
    "electron-builder": "^23.6.0",
    "electron-connect": "^0.6.3",
    "electron-packager": "^17.1.1",
    "electron-updater": "^5.3.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte": "^2.30.0",
    "factory.ts": "^1.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^5.0.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0",
    "vite": "^4.1.4"
  },
  "type": "module",
  "dependencies": {
    "@egjs/svelte-view360": "^4.0.0-beta.7",
    "@mdi/js": "^7.3.67",
    "@zoom-image/svelte": "^0.1.8",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "copy-image-clipboard": "^2.1.2",
    "dom-to-image": "^2.6.0",
    "electron-context-menu": "^3.6.1",
    "electron-log": "^5.0.0",
    "electron-nightly": "^29.0.0-nightly.20231110",
    "electron-reloader": "^1.2.3",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "handlebars": "^4.7.7",
    "justified-layout": "^4.1.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lodash-es": "^4.17.21",
    "luxon": "^3.2.1",
    "readdirp": "^3.6.0",
    "socket.io-client": "^4.6.1",
    "svelte-loading-spinners": "^0.3.4",
    "svelte-local-storage-store": "^0.5.0",
    "thumbhash": "^0.1.1"
  }
}
